{
  "manifest_version": 3,
  "name": "Block Web Preview",
  "version": "1.0",
  "description": "Blocks YouTube embeds in Google Docs previews",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "https://docs.google.com/*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}
